{
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "lines": 99,
  "produce-source-map": true,
  "statements": 99,
  "functions": 95,
  "branches": 95,
  "include": ["src/**/*.ts"],
  "exclude": ["**/*.spec.ts", "spec", "coverage"],
  "reporter": ["lcov", "text-summary", "html"],
  "cache": true,
  "all": true,
  "report-dir": "./coverage",
  "tmp-directory": ".nyc_output",
  "extension": [".ts"]
}
